        "add_drop_period": {
            "filename": "add_drop_period",
            "title": "Add/Drop Period",
            "type": "question",
            "descp": "",
            "info": "<p><b>Before swapping courses make sure to consult your adviser about the choice of course.</b><br />Is it outside the add/drop period? Dates can be found <a href=\"https://www.gla.ac.uk/myglasgow/registry/enrolment/#/makingchanges\">here.</a> <br><br> <h5>(The add/drop period is the first to weeks of teaching in the year. It finishes on the friday of the second week of teaching. It exists so that students aren't tied down to their course choices before the start of term and gives them the fexibility to change them.)</h5></p>",
            "subcats": [{
                "name": "Yes",
                "link": "goto_adviser",
                "linkexternal": false
            }, {
                "name": "No",
                "link": "compsci_teaching_office",
                "linkexternal": false
            }]
